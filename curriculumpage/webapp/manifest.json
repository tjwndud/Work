{
  "_version": "1.60.0",
  "sap.app": {
    "id": "code.cl3.curriculumpage",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.19.1",
      "toolsId": "c6350e9c-55eb-4634-843b-b38776000ccc"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.38",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "code.cl3.curriculumpage.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "flip",
        "type": "View",
        "viewType": "XML",
        "path": "code.cl3.curriculumpage.view",
        "async": true,
        "viewPath": "code.cl3.curriculumpage.view"
      },
      "routes": [
        {
          "name": "RouteCurriculum",
          "pattern": ":?query:",
          "target": [
            "TargetCurriculum"
          ]
        },
         {
          "name": "RouteCurriculum2",
          "pattern": "Curr2",
          "target": [
            "TargetCurriculum2"
          ]
        },
         {
          "name": "RouteCurriculum3",
          "pattern": "Curr3",
          "target": [
            "TargetCurriculum3"
          ]
        }
      ],
      "targets": {
        "TargetCurriculum": {
          "id": "Curriculum",
          "name": "Curriculum"
        },

        "TargetCurriculum2": {
          "id": "Curriculum2",
          "name": "Curriculum2"
        },
        "TargetCurriculum3": {
          "id": "Curriculum3",
          "name": "Curriculum3"
        }
        
      }
    },
    "rootView": {
      "viewName": "code.cl3.curriculumpage.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
